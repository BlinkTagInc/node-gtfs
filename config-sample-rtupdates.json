{
  "agencies": [
    {
      "url": "http://www.bart.gov/dev/schedules/google_transit.zip",
      "exclude": ["shapes"],
      "agency_headers": {
        "X-CustomHeader": "MyCustomHeader"
      },
      "rtupdates": {
        "trip_updates": "https://opendata.samtrafiken.se/gtfs-rt/sl/TripUpdates.pb?key=yourkey",
        "vehicle_positions": "https://opendata.samtrafiken.se/gtfs-rt/sl/VehiclePositions.pb?key=yourkey",
        "service_alerts": "https://opendata.samtrafiken.se/gtfs-rt/sl/ServiceAlerts.pb?key=yourkey",
        "occupancy_data": "https://opendata.samtrafiken.se/gtfs-rt/sl/OccupancyData.pb?key=yourkey",
        "update_headers": {
          "X-CustomHeader": "MyCustomHeader"
        }
      }
    },
    {
      "path": "/path/to/gtfs.zip",
      "exclude": ["stop_times", "shapes"]
    }
  ],
  "csvOptions": {
    "skip_lines_with_error": true
  },
  "sqlitePath": "/tmp/gtfs",
  "exportPath": "~/path/to/export/gtfs"
}
